{
  "expo": {
    "name": "kartikapp",
    "scheme": "kartik-app",
    "slug": "kartikapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.zoopercart.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.deepak.opticals"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
        "expo-system-ui"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "bba1d6ff-59a5-49b1-b45d-7f46b6f80f62"
      }
    }
  }
}
